{
  "name": "mp3gain.js",
  "version": "0.0.1",
  "description": "port of mp3gain in javascript, for use in browser",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ybootin/mp3gain.js.git"
  },
  "files": [
    "dist/*",
    "src/*",
    "typings/*",
    "demo/*",
    "README.md"
  ],
  "keywords": [
    "mp3",
    "audio",
    "normalize",
    "emscripten",
    "browser"
  ],
  "author": "Yohan Boutin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ybootin/mp3gain.js/issues"
  },
  "homepage": "https://github.com/ybootin/mp3gain.js#readme",
  "dependencies": {},
  "devDependencies": {
    "typescript": "^2.6.2",
    "emloader": "^0.0.6"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run buildts && npm run buildbinary",
    "buildts": "npm run buildmp3gain && npm run buildworker",
    "buildmp3gain": "node_modules/typescript/bin/tsc -d  --target es5 --sourceMap --outFile dist/mp3gain.js src/mp3gain/MP3gain.ts",
    "buildworker": "node_modules/typescript/bin/tsc -d  --target es5 --sourceMap --outFile dist/worker.js src/mp3gain/worker.ts",
    "buildbinary": "./build.sh",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
